<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <record id="partner_action" model="ir.actions.act_window">
        <field name="name">Contacts</field>
        <field name="res_model">re.partner</field>
        <field name="view_mode">kanban,tree,form</field>
    </record>

    <record id="agent_action" model="ir.actions.act_window">
        <field name="name">Agents</field>
        <field name="res_model">re.agent</field>
        <field name="view_mode">kanban,tree,form</field>
    </record>

    <record id="estate_action" model="ir.actions.act_window">
        <field name="name">Estate</field>
        <field name="res_model">re.estate</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

    <record id="contract_action" model="ir.actions.act_window">
        <field name="name">Contract</field>
        <field name="res_model">re.contract</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

        <record id="location_action" model="ir.actions.act_window">
        <field name="name">location</field>
        <field name="res_model">re.location</field>
        <field name="view_mode">tree,form,kanban</field>
    </record>

    <menuitem id="re_root_menu" name="Real Estate"/>
    <menuitem id="menu_estate_root" parent="re_root_menu" name="Estate" action="estate_action" sequence="1"/>
    <menuitem id="menu_contact_root" parent="re_root_menu" name="Contacts" sequence="2"/>
    <menuitem id="menu_partner_root" parent="menu_contact_root" name="Contacts" sequence="1" action="partner_action"/>
    <menuitem id="menu_agent_root" parent="menu_contact_root" name="Agents" sequence="2" action="agent_action"/>
    <menuitem id="menu_contract_root" parent="re_root_menu" name="Contracts" sequence="3" action="contract_action"/>
    <menuitem id="menu_location_root" parent="re_root_menu" name="location" sequence="4" action="location_action"/>

</odoo>